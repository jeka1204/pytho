import random
my_list = [random.randint(1, 10) for i in range(1, 10)]

list1 = random.choice(my_list)
list2 = random.choice(my_list)

print(list1)
print(list2)

a = []
randomlist = [[random.randint(1, 10) for i in range(0, list1)] for j in range(0, list2)]
print(randomlist)
print(sum(randomlist, []))
a.append(sum(sum(randomlist, [])))
print(a)
